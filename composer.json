{
    "name": "catalinbuletin/formz",
    "authors": [
        {
            "name": "Catalin Buletin",
            "email": "sanateria@gmail.coom"
        }
    ],
    "description": "Build forms in a fluent and easy way",
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.4",
        "dflydev/dot-access-data": "^2.0",
        "illuminate/support": "^7.2",
        "illuminate/view": "^7.4",
        "symfony/finder": "^5.0",
        "webmozart/assert": "^1.7"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "classmap": [

        ],
        "psr-4": {
            "Formz\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Formz\\ServiceProvider"
            ]
        }
    },
    "scripts": {

    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
