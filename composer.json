{
    "name": "catalinbuletin/formz",
    "description": "Laravel Form Builder",
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.4",
        "dflydev/dot-access-data": "^2.0",
        "illuminate/support": "^7.2",
        "symfony/finder": "^5.0",
        "webmozart/assert": "^1.7"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "classmap": [

        ],
        "psr-4": {
            "Formz\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {

    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
