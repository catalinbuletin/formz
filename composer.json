    "type": "project",
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "dflydev/dot-access-data": "^3.0",
        "illuminate/console": "^7.0|^8.0",
        "illuminate/http": "^7.0|^8.0",
        "illuminate/support": "^7.0|^8.0",
        "illuminate/view": "^7.0|^v8.0",
        "symfony/finder": "^5.0",
        "webmozart/assert": "^1.7"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "^1.3",
        "orchestra/testbench": "^5.1",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "classmap": [

        ],
        "psr-4": {
            "Formz\\": "src/"
